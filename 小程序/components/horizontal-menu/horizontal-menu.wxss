.horizontal-menu {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #14213D;
  height: 120rpx;
}

.menu-item {
  display: flex;
  width: 20%;
  flex-direction: column;
  align-items: center;
  margin: 0 20rpx;
}

.menu-icon {
  width: 40rpx;
  height: 40rpx;
}

.menu-label {
  margin-top: 8rpx;
  font-size: 24rpx;
  color: #FFF;
}
